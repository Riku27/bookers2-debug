<h2>Follower Users</h2>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
        <th></th>

      </tr>
    </thead>
    <tbody>
    <% if @users.exists? %>
     <% @users.each do |user| %>
     <tr>
       <td><%= image_tag user.get_profile_image, size: '50x50' %></td>
       <td><%= user.name %></td>
       <td>フォロー数:<%= user.followings.count %></td>
       <td>フォロワー数:<%= user.followers.count %></td>
       <td><%= link_to 'Show', user, class: "user_#{user.id}" %></td>
     </tr>
     <% end %>
     <% else %>
      <p>ユーザーはいません</p>
     <% end %>
    </tdoby>
  </table>